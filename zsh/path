# add go path to path
export GOPATH="/Users/ccauet/.go"
export PATH="${GOPATH//://bin:}/bin:$PATH"
# homebrew's sbin
export PATH="/usr/local/sbin:$PATH"
# add cabal to path
export PATH="/Users/ccauet/.cabal/bin:$PATH"
# add poetry package manager
export PATH="$HOME/.local/bin:$PATH"

